<!-- app/views/reports/new.html.erb -->
<div class="container">
  <h1>Upload Benchmarking Data</h1>
  <%= form_with url: reports_path, local: true, multipart: true do |form| %>
    <div class="form-group">
      <%= form.label :data_file, "Benchmarking Data File" %>
      <%= form.file_field :data_file, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= form.submit "Upload", class: "btn btn-primary" %>
    </div>
    <div id="app">
       <report-upload></report-upload>
    </div>
  <% end %>
</div>