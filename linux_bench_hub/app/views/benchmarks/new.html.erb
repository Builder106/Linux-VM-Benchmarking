<h1>New Benchmark</h1>
<%= form_with model: @benchmark, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :data %>
    <%= form.text_area :data %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
<%= link_to 'Back', benchmarks_path %>